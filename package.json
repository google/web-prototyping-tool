{
  "name": "web-prototyping-tool",
  "version": "0.3.0",
  "author": "Google LLC",
  "license": "Apache-2.0",
  "private": true,
  "sideEffects": true,
  "repository": {
    "type": "git"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean-webdriver": "webdriver-manager clean",
    "install-e2e-chrome": "webdriver-manager update --versions.chrome=86.0.4240.22",
    "appengine": "dev_appserver.py app.yaml",
    "build:all:dev-branch": "npm run build:all:libs:prod && npm run build:render-outlet:prod && npm run build:renderer:prod && npm run build:export-bundle && npm run build:app:dev-branch",
    "build:all:libs": "npm run build:interfaces && npm run build:meta && npm run build:utils && npm run build:themes && npm run build:common && npm run build:definitions",
    "build:all:libs:prod": "npm run build:interfaces && npm run build:meta && npm run build:utils:prod && npm run build:themes  && npm run build:common:prod && npm run build:definitions:prod",
    "build:all:master": "npm run build:all:libs:prod && npm run build:render-outlet:prod && npm run build:renderer:prod && npm run build:export-bundle && npm run build:app:master",
    "build:all:prod": "npm run build:all:libs:prod && npm run build:render-outlet:prod && npm run build:renderer:prod && npm run build:export-bundle && npm run build:app:prod",
    "build:all:screenshot-prod": "npm run build:all:libs:prod && npm run build:render-outlet:prod && npm run build:renderer:prod && npm run build:app:screenshot-prod",
    "build:all": "npm run build:all:libs && npm run build:render-outlet:prod && npm run build:renderer && npm run build:app",
    "build:app:dev-branch": "ng build --configuration=dev-branch",
    "build:app:master": "ng build --configuration=master",
    "build:app:prod": "ng build --configuration=production",
    "build:app:prod:stats": "ng build --configuration=production --stats-json",
    "build:app:screenshot-prod": "ng build --configuration=screenshot-prod",
    "build:app": "ng build",
    "build:builtins": "ng build cd-builtins",
    "build:common:prod": "ng build cd-common --configuration production",
    "build:component-demos": "ng build component-demos --configuration production --source-map=false --base-href='/component-demos/'",
    "build:common": "ng build cd-common",
    "build:export": "ng build cd-export --configuration production --ts-config ./projects/cd-export/tsconfig.app.prod.json --cross-origin=use-credentials --base-href='.' --source-map=false",
    "build:export-bundle": "npm run build:export && npm run zip:export",
    "build:definitions": "ng build cd-definitions",
    "build:definitions:prod": "ng build cd-definitions --configuration production",
    "build:interfaces": "ng build cd-interfaces",
    "build:meta": "ng build cd-metadata",
    "build:presence-service:prod": "webpack --config ./backend-services/presence/webpack.config.prod.js",
    "build:presence-service": "webpack --config ./backend-services/presence/webpack.config.js",
    "build:render-outlet:prod": "ng build render-outlet --configuration production --ts-config ./projects/render-outlet/tsconfig.app.prod.json --cross-origin=use-credentials --base-href='.'",
    "build:render-outlet": "ng build render-outlet --cross-origin=use-credentials --base-href='.'",
    "build:renderer:prod": "ng build renderer --configuration production --ts-config ./projects/renderer/tsconfig.app.prod.json --cross-origin=use-credentials --base-href='.'",
    "build:renderer": "ng build renderer --cross-origin=use-credentials --base-href='.'",
    "build:screenshot-service:prod": "webpack --config ./backend-services/screenshot/webpack.config.prod.js",
    "build:screenshot-service": "webpack --config ./backend-services/screenshot/webpack.config.js",
    "build:themes": "ng build cd-themes",
    "build:utils:prod": "ng build cd-utils --configuration production",
    "build:utils": "ng build cd-utils",
    "cloud-build-local": "cloud-build-local --config=cloudbuild.json --dryrun=false .",
    "code-review": "./scripts/code-review.sh",
    "copy-project-to-dev": "node ./scripts/copy-project-to-dev/index.js",
    "deploy:email-service": "gcloud app deploy ./backend-services/email-alerts/app.yaml",
    "deploy:email-service:prod": "gcloud app deploy ./backend-services/email-alerts/app.yaml",
    "deploy:presence-service": "gcloud app deploy ./dist/presence-service/app.yaml",
    "deploy:screenshot-service": "gcloud app deploy ./dist/app.yaml",
    "e2e:debug": "ng e2e --protractor-config ./e2e/protractor.debug.conf.js",
    "e2e:headless:parallel": "ng e2e --protractor-config ./e2e/protractor.headless-parallel.conf.js --configuration=e2e-headless",
    "e2e:headless": "ng e2e --protractor-config ./e2e/protractor.headless.conf.js --configuration=e2e-headless",
    "e2e:images": "ng e2e --protractor-config ./e2e/protractor.images.conf.js --configuration=e2e-headless",
    "e2e": "ng e2e",
    "firebase:deploy:functions": "cd firebase; firebase deploy --only functions",
    "license:check": "node scripts/license",
    "lint:changed": "git diff --cached --name-only --diff-filter=ACM | grep -E '\\.tsx?$' | xargs tslint --format codeFrame --config './tslint.json'",
    "lint:styles:app": "stylelint \"src/**/*.scss\" ",
    "lint:styles:common": "stylelint \"projects/cd-common/**/*.scss\" ",
    "lint": "ng lint",
    "ng": "ng",
    "postversion": "git commit --amend -m 'Publish (NPM) version: '$npm_package_version && git push && git push --tags",
    "prettier:all": "prettier --config ./.prettierrc  --write \"./{src,projects,firebase,backend-services}/**/*{.ts,.scss,.html}\"",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"./{src,projects}/**/*{.ts,.scss}\"",
    "prettier:fix": "pretty-quick --staged",
    "show-build-triggers": "GOOGLE_APPLICATION_CREDENTIALS=key.json ts-node ./cloud-builds/triggers/index.ts show-remote",
    "sourcetree": "open /Applications/SourceTree.app & exit 0",
    "component-demos": "ng serve component-demos --port=9000",
    "start:export": "ng serve cd-export",
    "start:live-renderer": "ng serve --configuration=live-renderer & ng serve renderer --configuration=live-renderer",
    "start:project-services": "GOOGLE_APPLICATION_CREDENTIALS=key.json node ./dist/backend-services/project-services/index.js",
    "start:screenshot-server": "cd dist && GOOGLE_APPLICATION_CREDENTIALS=../key.json node ./screenshot-service.js",
    "start:watch": "npm run start & npm run watch:utils",
    "start": "ng serve",
    "start:prod-debug": "ng serve --configuration=production-debug",
    "test:app:headless": "ng test web-prototyping-tool --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:app": "ng test web-prototyping-tool --browsers=Chrome",
    "test:common:headless": "ng test cd-common --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:common": "ng test cd-common --browsers=Chrome",
    "test:definitions:headless": "ng test cd-definitions --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:definitions": "ng test cd-definitions --browsers=Chrome",
    "test:full": "npm run build:all:prod && npm run test:headless && npm run e2e:headless",
    "test:headless": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI --source-map=false",
    "test:renderer:headless": "ng test renderer --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:renderer": "ng test renderer --browsers=Chrome",
    "test:render-outlet:headless": "ng test render-outlet --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:render-outlet": "ng test render-outlet --browsers=Chrome",
    "test:themes:headless": "ng test cd-themes --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:themes": "ng test cd-themes --browsers=Chrome",
    "test:utils:headless": "ng test cd-utils --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:utils": "ng test cd-utils --browsers=Chrome",
    "test": "ng test --browsers=Chrome",
    "update-build-triggers:all": "GOOGLE_APPLICATION_CREDENTIALS=key.json ts-node ./cloud-builds/triggers/index.ts update-all",
    "update-build-triggers": "GOOGLE_APPLICATION_CREDENTIALS=key.json ts-node ./cloud-builds/triggers/index.ts update",
    "watch:common": "ng build cd-common --watch",
    "watch:interfaces": "ng build cd-interfaces --watch",
    "watch:meta": "ng build cd-metadata --watch",
    "watch:renderer": "ng build renderer --watch",
    "watch:themes": "ng build cd-themes --watch",
    "watch:utils": "ng build cd-utils --watch",
    "zip:export": "cd ./dist; bestzip cd-export.zip cd-export/",
    "analyize-bundle": "webpack-bundle-analyzer dist/web-prototyping-tool/stats.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:fix"
    }
  },
  "dependencies": {
